{
  "name": "lolscouter",
  "version": "0.1.0",
  "private": true,
  "main": "electron/dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"yarn next:dev\" \"yarn electron:dev\"",
    "next:dev": "next dev",
    "electron:dev": "tsc -p tsconfig.electron.json && electron .",
    "build": "next build && tsc -p tsconfig.electron.json && electron-builder",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "framer-motion": "^12.6.5",
    "league-connect": "^6.0.0-rc13",
    "next": "15.2.4",
    "openai": "^4.91.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/electron": "^1.6.10",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^8.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "build": {
    "appId": "com.yourapp.lolscouter",
    "files": [
      "out/**/*",
      "electron/dist/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
